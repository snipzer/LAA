{
    "extends": "airbnb-base",
    "rules": {
        "indent": ["error", 4],
        "class-methods-use-this": [0],
        "linebreak-style":"off",
        "comma-dangle": "off",
        "no-param-reassign": "off",
        "arrow-body-style": "off",
        "no-underscore-dangle": "off",
        "max-len": ["error", { "code": 180 }]
    }
}
